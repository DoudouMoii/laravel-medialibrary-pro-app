{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "link-js": "yarn link @spatie/medialibrary-pro-core @spatie/medialibrary-pro-vue @spatie/medialibrary-pro-react @spatie/medialibrary-pro-vue-table @spatie/medialibrary-pro-react-table @spatie/medialibrary-pro-vue-list @spatie/medialibrary-pro-react-list @spatie/medialibrary-pro-vue-single @spatie/medialibrary-pro-react-single",
        "setup-js": "cd .. && git clone git@github.com:spatie/medialibrary-pro-js.git ; cd medialibrary-pro-js && git pull && yarn setup",
        "setup-php": "cd .. && git clone git@github.com:spatie/laravel-medialibrary-pro.git ; cd laravel-medialibrary-pro && git pull && composer install && cd .. && git clone git@github.com:spatie/laravel-medialibrary.git ; cd laravel-medialibrary && git pull && composer install",
        "setup": "yarn setup-js && yarn setup-php && yarn && yarn link-js && yarn watch"
    },
    "dependencies": {
        "laravel-vapor": "^0.4.0",
        "lodash": "^4.17.13",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "tailwindcss": "^1.2.0",
        "vue": "^2.6.11"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.8.3",
        "@types/react": "^16.9.22",
        "@types/react-dom": "^16.9.5",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.2",
        "vue-template-compiler": "^2.6.11"
    }
}
